<link rel="stylesheet" href="/styles/fileviewer.css">

<main>

    <form id="searchform">

        <input type="text" name="searchbar" id="searchbar" placeholder="Search here..">
        <button  id="btn" type="submit">
            <i class="fa-solid fa-magnifying-glass"></i>
            Search
        </button>

    </form>

<table>
    <thead>
        <tr>
        <% for(let h of head) { %>
            <th><%= h %></th>
        <% } %>
        </tr>
    </thead>
    <tbody>
        <% for(let row of data) { %>
        <tr>
            <% for(let key of Object.keys(row)) { %>
                <td><%= row[key] %></td>
            <% } %>
        </tr>
        <% } %>
    </tbody>
</table>
    
</main>

<script src="/scripts/fileupload.js"></script>